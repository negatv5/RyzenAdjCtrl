QWidget {
background-color: rgb(190, 0, 55);
}
QPushButton {
color: rgb(255,255,255);
border: 1px solid gray;
background-color: rgb(58,57,58);
border-radius: 0px;
}
QPushButton:hover {
background-color: rgb(96, 96, 96);
}
QPushButton:pressed {
background-color: rgb(128, 128, 128);
}
QPushButton:checked {
background-color: rgb(192, 192, 192);
}